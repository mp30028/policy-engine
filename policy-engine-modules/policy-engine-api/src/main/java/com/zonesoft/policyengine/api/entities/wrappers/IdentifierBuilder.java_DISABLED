package com.zonesoft.policyengine.api.entities.wrappers;

public class IdentifierBuilder <T extends Identifier> implements Identifier {
	private final T entity;
	
	public IdentifierBuilder(T entity) {
		super();
		this.entity = entity;
	}
	
	@Override
	public Long getId() {
		return entity.getId();
	}	
	
	@Override
	public String getName() {
		return entity.getName();
	}
}
